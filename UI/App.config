<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  
  <!--Declarando a camada da appSettings no projeto-->
  <!--Em outras palavras tudo que esta dentro do appSettings sera como variaveis de ambiente-->
  
  <!--alias nunca vi string de conexao em camada de interface kkkkkkkk mas tabom ne cada um com suas loucuras--> 
  <appSettings>
    
    <add key="DataSource" value="server=localhost;user=root;password=1234;database=flower;persistsecurityinfo=True;port=3306" />
  
  </appSettings>

  <runtime>
